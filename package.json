{
  "name": "starter",
  "version": "1.0.0",
  "description": "Landing page of udemy-css",
  "main": "index.js",
  "scripts": {
    "build:css": "sass sass/main.scss css/style.css -w",
    "compile:css": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-fonts.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css:css/style.min.css  --style compressed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-run": "^5.0.1",
    "sass": "^1.71.1"
  },
  "devDependencies": {
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5"
  }
}